{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "dom",
      "es5",
      "scripthost",
      "es2015",
      "esnext.asynciterable"
    ],
    "sourceMap": true,
    "noImplicitAny": false,
    "allowJs": true
  },
  "exclude": [
    "node_modules"
  ],
  "files": [
    "./src/*.d.ts",
    "./src/*.ts",
    "./src/lib/*.ts",
    "./src/lib/*/*.ts",
    "./src/lib/*/*/*.ts",
    "./src/lib/*/*/*/*.ts",
    "./src/lib/*/*/*/*/*.ts",
    "./src/lib/*/*/*/*/*/*.ts",
    "./src/lib/*/*/*/*/*/*/*.ts"
  ]
}